{
 "name": "dariah-topics-app",
 "productName": "DARIAH Topics",
 "version": "0.2.0",
 "main": "main.js",
 "devDependencies": {
 "electron": "^1.4.3",
 "electron-packager": "^8.1.0"
 },
 "dependencies": {
 "request-promise": "^4.2.2"
 },
 "scripts": {
 "package-mac": "electron-packager dist --overwrite --platform=darwin --arch=x64 --icon=static/img/app_icon.png --prune=true --out=release-builds",
 "package-win": "electron-packager dist dariah-topics-app --overwrite --platform=win32 --arch=ia32 --icon=static/img/app_icon.ico --prune=true --out=release-builds --version-string.CompanyName=CE --version-string.FileDescription=CE --version-string.ProductName=\"DARIAH Topics\"",    
 }
}
